  "devDependencies": {
    "electron": "^36.9.1",
    "mocha": "^10.8.2",
    "node-gyp": "11.4.2"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "nan": "2.23.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu-Bashir/electron-panel-window.git"
  },
  "bugs": {
    "url": "https://github.com/Ashu-Bashir/electron-panel-window/issues"
  },
  "gypfile": true,
  "homepage": "https://github.com/Ashu-Bashir/electron-panel-window#readme",
